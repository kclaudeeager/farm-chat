# Farm Control Demo Environment Configuration
# Copy this file to .env.docker and set your actual API keys

# GROQ API Key (required for AI responses)
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API Key (for enhanced functionality)
OPENAI_PROJECT_API_KEY=your_openai_api_key_here

# ThingsBoard Configuration
THINGSBOARD_HOST=thingsboard
